{
  "name": "node-es6-boilerplate",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "description": "NodeJS boilerplate with mongodb stack",
  "main": "server.js",
  "author": {
    "name": "bogardt"
  },
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js controllers/ models/ modules/ routes/ test/",
    "populate": "./node_modules/.bin/babel-node ./scripts/populate.user.js",
    "pretest": "npm run populate",
    "start": "./node_modules/nodemon/bin/nodemon.js --exec babel-node server.js",
    "test": "./node_modules/.bin/mocha --require babel-core/register test/*.spec.js",
    "unpopulate": "./node_modules/.bin/babel-node ./scripts/unpopulate.user.js"
  },
  "keywords": [
    "node",
    "nodejs",
    "es6",
    "boilerplate",
    "docker-compose",
    "mongodb",
    "babel-es6",
    "babel",
    "skeleton",
    "api"
  ],
  "dependencies": {
    "bcrypt": "5.0.0",
    "body-parser": "1.18.3",
    "cors": "2.8.5",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "joi": "14.3.0",
    "jsonwebtoken": "8.4.0",
    "lint": "1.1.2",
    "mongoose": "5.3.15",
    "morgan": "1.9.1",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "swagger-ui-express": "^4.0.2",
    "winston": "2.4.4",
    "winston-daily-rotate-file": "1.7.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.6",
    "babel-preset-node6": "11.0.0",
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "eslint": "5.10.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.8",
    "prettier": "1.15.3"
  }
}
