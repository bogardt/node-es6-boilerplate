{
  "name": "node-es6-boilerplate-docker",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "description": "NodeJS boilerplate with mongodb stack",
  "main": "server.js",
  "author": {
    "name": "bogardt"
  },
  "scripts": {
    "start": "./node_modules/nodemon/bin/nodemon.js --exec babel-node server.js",
    "populate": "./node_modules/.bin/babel-node ./scripts/populate.user.js",
    "unpopulate": "./node_modules/.bin/babel-node ./scripts/unpopulate.user.js",
    "pre-test": "./node_modules/.bin/mocha --require babel-core/register test/*.spec.js",
    "lint": "./node_modules/eslint/bin/eslint.js controllers/ models/ modules/ routes/ test/",
    "test": "npm run populate && npm run pre-test"
  },
  "keywords": [
    "node",
    "nodejs",
    "es6",
    "boilerplate",
    "docker-compose",
    "mongodb",
    "babel-es6",
    "babel",
    "skeleton",
    "api"
  ],
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "express": "^4.16.2",
    "joi": "^14.3.0",
    "jsonwebtoken": "^8.3.0",
    "lint": "^1.1.2",
    "mongoose": "^5.2.8",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "swagger-tools": "^0.10.4",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-node6": "^11.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.8",
    "prettier": "^1.14.3"
  }
}